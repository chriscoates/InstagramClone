<h1>Index</h1>

<div class="posts-wrapper row">
  <% @posts.each do |post| %>
  <div class="post">
    <div class="post-head">

    </div>
    <div class="image center-block">
      <%= link_to (image_tag post.image.url(:medium), class:'img-responsive'), post_path(post) %>
    </div>
    <p class="caption">
      <%= post.caption %>
    </p>
  </div>
  <% end %>
</div>


<%= link_to "New Post", new_post_path %>